{
	"name": "packmorph",
	"version": "1.0.0",
	"description": "Translate package manager install commands between npm, pnpm, Yarn, and Bun",
	"type": "module",
	"main": "./src/index.ts",
	"module": "./src/index.ts",
	"exports": {
		".": {
			"import": "./src/index.ts",
			"types": "./src/index.ts"
		}
	},
	"files": [
		"src"
	],
	"scripts": {
		"test": "bun test",
		"format": "biome format --write",
		"lint": "biome lint --write",
		"check": "biome check --write",
		"ci": "biome ci"
	},
	"keywords": [
		"package-manager",
		"npm",
		"pnpm",
		"yarn",
		"bun",
		"converter",
		"translate"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/RostiMelk/packmorph.git"
	},
	"bugs": {
		"url": "https://github.com/RostiMelk/packmorph/issues"
	},
	"homepage": "https://github.com/RostiMelk/packmorph#readme",
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "2.3.6",
		"@types/bun": "latest"
	},
	"peerDependencies": {
		"typescript": "^5"
	}
}
